@import "../../tools/all.pcss";

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  animation: fadeIn 1.5s ease;
}

.list-item {
  display: block;
  color: $black;
  flex-grow: 1;
  margin: 20px;
  padding: 10px;
  min-width: 200px;
  max-width: 290px;
  box-shadow: 0 2px 5px rgba(0,0,0,0.26)
}
