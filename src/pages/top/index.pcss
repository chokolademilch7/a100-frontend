@import "../../tools/helper.pcss";
@import "../../tools/color.pcss";

.test {
  color: $orange;
}

.title {
  margin: 40px 0 30px;
}

.title__main {
  width: 280px;
  margin: 16px auto;
}

.title__sub {
  width: 260px;
  margin: 0 auto 20px
}

.title__logo {
  width: 140px;
  margin: 0 auto;
}

.main {
  display: flex;
  width: 80%;
  margin: 0 auto 4%;
}

.main__contents {
  width: 33%;
}

.main__item {
  height: 26%;
  margin: 16px 8px;
  border-radius: 20px;
  overflow: hidden;
}

.main__image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

a100-button {
  display: block;
  width: 200px;
  margin: 0 auto;
}
